@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<https://data.vlaanderen.be/id/conceptscheme/postinfostatus>
    a skos:ConceptScheme ;
    skos:prefLabel "conceptscheme postinfostatus" ;
    skos:definition "Codelijst voor de status van een postinfo." .
    
<https://data.vlaanderen.be/id/concept/postinfostatus/gehistoreerd>    
     a skos:Concept ;
     skos:definition "De postinfocode werd gehistoreerd door de aanbieder van de universele postdienst." ;
     skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/postinfostatus> ;
     skos:prefLabel "gehistoreerd" ;
     skos:relatedMatch <https://inspire.ec.europa.eu/codelist/StatusValue/retired> ;
     skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/postinfostatus> .
     
<https://data.vlaanderen.be/id/concept/postinfostatus/gerealiseerd>    
     a skos:Concept ;    
     skos:definition "De postinfocode werd in gebruik genomen door de aanbieder van de universele postdienst" ;
     skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/postinfostatus> ; 
     skos:prefLabel "gerealiseerd" ;
     skos:relatedMatch <https://inspire.ec.europa.eu/codelist/StatusValue/current> ;
     skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/postinfostatus> .

